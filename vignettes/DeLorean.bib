	shortjournal = {Cell. Mol. Life Sci.},
	author = {Liu, Na and Liu, Lin and Pan, Xinghua},
	urldate = {2015-04-30},
	date = {2014-03-21},
	langid = {english},
	keywords = {Biochemistry, general, Biomedicine general, Cancer stem cells, Cell biology, Embryonic stem cells, Life Sciences, general, {RNA}-seq, Single cells, transcriptome},
	file = {Full Text PDF:/home/john/.mozilla/firefox/d39h8gpz.default/zotero/storage/2RUNRE7J/Liu et al. - 2014 - Single-cell analysis of the transcriptome and its .pdf:application/pdf;Snapshot:/home/john/.mozilla/firefox/d39h8gpz.default/zotero/storage/N8VEPCSF/10.html:text/html}
}


	author = {Nishioka, Noriyuki and Inoue, Ken-ichi and Adachi, Kenjiro and Kiyonari, Hiroshi and Ota, Mitsunori and Ralston, Amy and Yabuta, Norikazu and Hirahara, Shino and Stephenson, Robert O. and Ogonuki, Narumi and Makita, Ryosuke and Kurihara, Hiroki and Morin-Kensicki, Elizabeth M. and Nojima, Hiroshi and Rossant, Janet and Nakao, Kazuki and Niwa, Hitoshi and Sasaki, Hiroshi},
	urldate = {2015-05-14},
